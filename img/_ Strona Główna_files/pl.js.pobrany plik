(function(global){var Language = {};

Language.pluralFormFunction = function (n) {
		if (n == 1) {
			return 'one';
		}

		if (n == (n | 0) && n % 10 >= 2 && n % 10 <= 4 && (n % 100 < 12 || n % 100 > 14)) {
			return 'few';
		}

		return 'other';
	};


