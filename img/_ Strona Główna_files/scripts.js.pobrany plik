let refferer = setInterval(() => {
  if (typeof window !== "undefined") {
    clearInterval(refferer)
    window.sessionStorage.getItem("refferer")
      ? null
      : window.sessionStorage.setItem("refferer", document.referrer)
  }
}, 100)
